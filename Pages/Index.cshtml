@page
@model IndexModel

<h2>Kantinens Menu</h2>
<table>
    <thead>
        <tr>
            <th>Dag</th>
            <th>Varm Ret</th>
            <th>Kold Ret</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.MenuItems)
        {
            <tr>
                <td>@item.Dag</td>
                <td>@item.VarmRet</td>
                <td>@item.KoldRet</td>
            </tr>
        }
    </tbody>
</table>
